  <ItemGroup>
    <None Include="..\..\..\..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PrimeFuncPack.Core.Functionals.Pipelines" Version="1.0.0-preview.1.0.*" />
    <PackageReference Include="PrimeFuncPack.Core.Functionals.Primitives" Version="1.0.0-preview.1.0.*" />
    <PackageReference Include="PrimeFuncPack.Core.Infrastructure" Version="1.0.0-preview.1.3.*" />
